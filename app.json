{
  "expo": {
    "name": "Deuce League",
    "slug": "deuce-league",
    "version": "1.0.0",
    "owner": "nexea",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "deuceleague",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby leagues and courts.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby leagues and courts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.dlfrontend",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
        [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "targetSdkVersion": 34,
            "compileSdkVersion": 36
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Deuce League to use your location to find nearby courts and leagues.",
          "locationAlwaysPermission": "Allow Deuce League to use your location to find nearby courts and leagues.",
          "locationWhenInUsePermission": "Allow Deuce League to use your location to find nearby courts and leagues."
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e50b53ee-0b2f-4246-8e17-18865e579166"
      }
    }
  }
}
